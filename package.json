{
    "name": "modata-api",
    "version": "0.0.1",
    "description": "Test API",
    "main": "dist",
    "scripts": {
        "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
        "build": "babel src -s -D -d dist --presets es2015,stage-0",
        "start": "node dist",
        "prestart": "npm run -s build",
        "test": "eslint src",
        "debug": "node --inspect src/index.js"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaVersion": 7,
            "sourceType": "module"
        },
        "env": {
            "node": true
        },
        "rules": {
            "no-console": 0,
            "no-unused-vars": 1
        }
    },
    "author": "Setty",
    "dependencies": {
        "bluebird": "^3.7.2",
        "body-parser": "^1.13.3",
        "compression": "^1.5.2",
        "cors": "^2.7.1",
        "express": "^4.13.3",
        "moment-timezone": "^0.5.31",
        "mongoose": "^5.10.0",
        "morgan": "^1.8.0",
        "resource-router-middleware": "^0.6.0",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.0"
    },
    "devDependencies": {
        "babel-cli": "^6.9.0",
        "babel-core": "^6.9.0",
        "babel-preset-es2015": "^6.9.0",
        "babel-preset-stage-0": "^6.5.0",
        "eslint": "^3.1.1",
        "nodemon": "^1.9.2"
    }
}